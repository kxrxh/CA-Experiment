.data
    hello: "Hello, \0" 
    
.text
    add r1, r0, hello // Load address to r1
    add r11, r0, in
    add r10, r0, out
loop:
    lw r2, r1 // Load 32 bits (WORD) by address (r1) to r2

    beq r2, r0, name // Check null char

    sw r2, r10

    add r1, r1, #1 // Get address of next memory cell

    jmp loop
name:

    lw r2, r11

    beq r2, r0, exit

    sw r2, r10

    jmp name

exit:
    halt

